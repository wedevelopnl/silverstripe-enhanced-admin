{
    "name": "wedevelopnl/silverstripe-enhanced-admin",
    "description": "A SilverStripe module that adds enhancements to the default admin",
    "keywords": [
        "silverstripe",
        "enhancements",
        "admin",
        "cms"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "WeDevelop",
            "email": "development@wedevelop.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2",
        "silverstripe/asset-admin": "^2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.20",
        "syntro/silverstripe-phpstan": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "WeDevelop\\EnhancedAdmin\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    }
}
